
<%= form_with scope: :customer_form ,local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :title %>
  </p>
 <p>
    <%= form.label :date_0f_birth %><br>
    <%= form.date_field :title %>
  </p>
   <p>
    <%= form.label :Photo %><br>
    <%= form.file_field :image %>
  </p>
  <p>
    <%= form.label :Address %><br>
    <%= form.text_area :text %>
  </p>
  <p>
    <%= form.label :Phone_no %><br>
    <%= form.number_field :text %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>